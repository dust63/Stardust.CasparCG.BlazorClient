@page "/records"
@inject IStringLocalizer<App> local
@using AntDesign.TableModels
@using System.ComponentModel




<GridContent>
    <Row Type="flex" Gutter="24">
        <Col Xs="24" Sm="12" Lg="12" Xl="6">
        <Card Actions="@(new[] { obsStatus((IsRecording, "Record Status")), obsStatus((IsStreaming, "Streaming Status")) })" Style="margin-bottom:24px">
            <Skeleton Loading="loading" Avatar Active>
                <CardMeta Title="OBS Studio"
                          Description="@local["YoutubeStatus"]"
                          Avatar="data:image/png;base64," />

            </Skeleton>
        </Card>
        </Col>
        <Col Xs="24" Sm="12" Lg="12" Xl="6">
        <Card Actions="@(new[] { iconText(("eye",YoutubeView)),iconText(("like",YoutubeSubsciption)),iconText(("folder-view",YoutubeVideoCount)) })" Style="margin-bottom:24px">
            <Skeleton Loading="loading" Avatar Active>
                <CardMeta Title="Youtube"
                          Description="@local["YoutubeStatus"]"
                          Avatar="data:image/png;base64," />
            </Skeleton>
        </Card>
        </Col>
        <Col Xs="24" Sm="12" Lg="12" Xl="6">
        <Card Actions="new[] { actionSetting, actionEdit, actionEllipsis }" Style="margin-bottom:24px">
            <Skeleton Loading="loading" Avatar Active>
                <CardMeta Title="Events"
                          Description=""
                          Avatar="https://zos.alipayobjects.com/rmsportal/ODTLcjxAfvqbxHnVXCYX.png" />
            </Skeleton>
        </Card>
        </Col>
    </Row>

    <Card>
        <CardTabs>
            <Tabs TabBarStyle="margin-bottom: 24px;">
                <TabPane Key="1">
                    <Tab>Event Scheduling</Tab>
                    <ChildContent>
                        <Row>
                            <Col Xs="24" Sm="24" Lg="24" Xl="24">
                            <Space Style="margin-bottom:24px">
                                <SpaceItem>
                                    <Button Type="primary" OnClick="OnRecord">
                                        <IconFont Type="icon-record" />
                                        Record
                                    </Button>
                                </SpaceItem>
                                <SpaceItem>
                                    <Button>
                                        <Icon Type="schedule" Theme="outline" OnClick="OnAddSchedule" /> Schedule
                                    </Button>
                                </SpaceItem>
                                @if (SelectedEventRows != null && SelectedEventRows.Any())
                                {
                                    <SpaceItem>
                                        <Button Danger OnClick="OnDeleteEvent" Icon="delete">Delete</Button>

                                    </SpaceItem>
                                }

                            </Space>

                            <Table @ref="table" TItem="ScheduledRecord" DataSource="@RecuringRecord" PageSize="10" ScrollX="1500" ScrollY="300" @bind-SelectedRows="SelectedEventRows">
                                <Selection Key="@(context.Id)" Width="40" Fixed="left" />
                                <AntDesign.Column @bind-Field="@context.Title" Width="100" Fixed="left" />
                                <AntDesign.Column @bind-Field="@context.Description" Width="100" Fixed="left" />
                                <AntDesign.Column @bind-Field="@context.DayOfWeek" Width="150" Title="Column 1" />
                                <AntDesign.Column @bind-Field="@context.StartTime" Width="150" Title="Column 2" />
                                <AntDesign.Column @bind-Field="@context.EndTime" Width="150" Title="Column 3" />
                                <ActionColumn Title="Action" Width="100" Fixed="right">
                                    <Icon Type="edite" OnClick="OnEditEvent"></Icon>
                                </ActionColumn>
                            </Table>
                            </Col>
                        </Row>
                    </ChildContent>
                </TabPane>
                <TabPane Key="2">
                    <Tab>Workflow</Tab>
                    <ChildContent>

                    </ChildContent>
                </TabPane>
            </Tabs>
        </CardTabs>
    </Card>

</GridContent>




@code{


    RenderFragment titleTemplate =@<div><Icon Type="obs" Theme="outline" />Obs</div>;

private bool loading = true;


public bool IsStreaming { get; set; }

public bool IsRecording { get; set; }

public int YoutubeView { get; set; }

public int YoutubeSubsciption { get; set; }

public int YoutubeVideoCount { get; set; }



protected override async Task OnInitializedAsync()
{
await Task.Delay(1000);
loading = false;
}

private RenderFragment<(bool isOnAir, string title)> obsStatus = context => @<Template>
    <div class="ant-statistic-title">@context.title</div>
    @if (context.isOnAir)
    {
        <AntDesign.Title Level="4" Type="danger">ON AIR</AntDesign.Title>
    }
    else
    {
        <Title Level="4">STOPPED</Title>
    }
</Template>;

RenderFragment<(string icon, int text)> iconText = context =>
@<Template>
    <Space>
        <SpaceItem><Icon Type="@context.icon" /></SpaceItem>
        <SpaceItem>@context.text</SpaceItem>
    </Space>
</Template> ;


private RenderFragment actionSetting =@<Template>
    <Icon Type="settings" />
</Template>;

private RenderFragment actionEdit =@<Template>
    <Icon Type="edit" />
</Template>;

private RenderFragment actionEllipsis =@<Template>
    <Icon Type="ellipsis" />
</Template>;

ITable table;

public IEnumerable<ScheduledRecord> SelectedEventRows { get; set; }

public List<ScheduledRecord> RecuringRecord { get; set; } = new List<ScheduledRecord> { new ScheduledRecord { DayOfWeek = DayOfWeek.Monday, Title = "Test", Description = "Description" } };


public void OnDeleteEvent()
{
SelectedEventRows = null;
}

public void OnEditEvent()
{

}

public void OnRecord()
{

}

public void OnAddSchedule()
{

}

}
